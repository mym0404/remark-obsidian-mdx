{
	"name": "remark-obsidian-mdx",
	"version": "1.11.1",
	"type": "module",
	"main": "dist/cjs/index.cjs",
	"module": "dist/esm/index.mjs",
	"types": "dist/esm/index.d.ts",
	"files": [
		"dist/cjs/index.cjs",
		"dist/esm/index.mjs",
		"dist/esm/index.d.ts"
	],
	"exports": {
		".": {
			"types": "./dist/esm/index.d.ts",
			"require": "./dist/cjs/index.cjs",
			"import": "./dist/esm/index.mjs"
		}
	},
	"scripts": {
		"test": "vitest run",
		"release": "release-it",
		"prepublishOnly": "npm run build",
		"build": "tsdown",
		"watch": "tsdown --watch",
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"lint": "biome check",
		"format": "biome check --write --unsafe",
		"t": "pnpm lint && pnpm typecheck"
	},
	"author": "MJ Studio",
	"description": "Remark plugin to support Obsidian markdown syntax",
	"repository": {
		"type": "git",
		"url": "https://github.com/mym0404/remark-obsidian-mdx.git"
	},
	"keywords": [
		"remark",
		"obsidian",
		"fumadocs",
		"mdx"
	],
	"publishConfig": {
		"access": "public"
	},
	"license": "GPL-3.0",
	"devDependencies": {
		"@biomejs/biome": "2.3.11",
		"@types/mdast": "^4.0.4",
		"@types/unist": "^3.0.3",
		"mdast-util-gfm-footnote": "^1.0.2",
		"mdast-util-gfm-strikethrough": "^1.0.3",
		"mdast-util-to-markdown": "^1.5.0",
		"mdast-util-to-string": "^3.2.0",
		"mdast-util-wiki-link": "^0.1.2",
		"micromark-extension-mark": "^1.0.2",
		"micromark-extension-wiki-link": "^0.0.4",
		"release-it": "^19.2.3",
		"remark": "^14.0.3",
		"remark-comment": "^1.0.0",
		"remark-frontmatter": "^4.0.1",
		"remark-gfm": "^3.0.1",
		"remark-html": "^15.0.2",
		"remark-parse": "^10.0.2",
		"slugify": "^1.6.6",
		"tsdown": "^0.19.0",
		"typescript": "^5.9.3",
		"unified": "^10.1.2",
		"unist-util-visit": "^4.1.2",
		"vitest": "^4.0.16",
		"yaml": "^2.3.1"
	},
	"packageManager": "pnpm@10.27.0+sha512.72d699da16b1179c14ba9e64dc71c9a40988cbdc65c264cb0e489db7de917f20dcf4d64d8723625f2969ba52d4b7e2a1170682d9ac2a5dcaeaab732b7e16f04a"
}
